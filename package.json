{
  "name": "tubugl-camera",
  "version": "1.4.7",
  "description": "camera for tubugl",
  "cjs": "build/tubu-camera.cjs.js",
  "esm": "build/tubu-camera.esm.js",
  "main": "build/tubu-camera.js",
  "libName": "Tubu",
  "license": "MIT",
  "scripts": {
    "app00": "parcel examples/app00/index.html --no-hmr",
    "dist00": "parcel build examples/app00/index.html -d docs/app00 --public-url ./",
    "screenshot00": "node ./node/screenshot.js --dir=app00",
    "app01": "parcel examples/app01/index.html --no-hmr",
    "dist01": "parcel build examples/app01/index.html -d docs/app01 --public-url ./",
    "screenshot01": "node ./node/screenshot.js --dir=app01",
    "app02": "parcel examples/app02/index.html --no-hmr",
    "dist02": "parcel build examples/app02/index.html -d docs/app02 --public-url ./",
    "screenshot02": "node ./node/screenshot.js --dir=app02",
    "docs": "python -m SimpleHTTPServer 8000",
    "build": "rollup -c"
  },
  "dependencies": {
    "gl-matrix": "^2.4.0",
    "tubugl-core": "^1.5.0",
    "tubugl-math": "^1.2.1",
    "tubugl-utils": "^1.1.2",
    "wolfy87-eventemitter": "^5.2.4"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "gsap": "^1.20.3",
    "minimist": "^1.2.0",
    "puppeteer": "^0.13.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "stats.js": "mrdoob/stats.js",
    "tubugl-3d-shape": "^1.6.1"
  }
